# Environment variables for 3D Model Generation Application

# Flask Configuration
SECRET_KEY=your-secret-key-here
FLASK_ENV=development

# Database
DATABASE_URL=sqlite:///model_generation.db

# API Keys
MESHY_AI_API_KEY=your-meshy-ai-api-key-here

# Logging
LOG_LEVEL=INFO

# Cache Settings
CACHE_SIMILARITY_THRESHOLD=0.85
CACHE_MAX_SIZE_GB=10

# Rate Limiting
RATE_LIMIT_PER_MINUTE=10
RATE_LIMIT_PER_HOUR=100

# File Upload Limits
MAX_IMAGE_SIZE_MB=10
MAX_TEXT_LENGTH=1000